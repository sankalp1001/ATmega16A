#define F_CPU 1000000UL
#include <avr/io.h>
void Delay(int n)
{
	for(int i=0;i<3*n;i++)
	{
		while((TIFR & 0x01)==0);
		TIFR=0x01;
	}
}

int main(void)
{
	DDRB=0x01;
	TCCR0=0x05;
	while (1)
	{
		PORTB=0x01;
		Delay(1);
		PORTB=0x00;
		Delay(1);
	}
}
